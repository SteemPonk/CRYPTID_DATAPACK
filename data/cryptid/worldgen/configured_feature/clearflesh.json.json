{
  "type": "minecraft:disk",
  "config": {
    "state_provider": {
      "fallback": {
        "type": "minecraft:simple_state_provider",
        "state": {
          "Name": "minecraft:air"
        }
      },
      "rules": [
        {
          "if_true": {
            "type": "minecraft:matching_blocks",
            "blocks": "minecraft:nether_wart_block"
          },
          "then": {
            "type": "minecraft:simple_state_provider",
            "state": {
              "Name": "minecraft:air"
            }
          }
        },
        {
          "if_true": {
            "type": "minecraft:matching_blocks",
            "blocks": "minecraft:crimson_nylium"
          },
          "then": {
            "type": "minecraft:simple_state_provider",
            "state": {
              "Name": "minecraft:grass_block"
            }
          }
        },
        {
          "if_true": {
            "type": "minecraft:matching_blocks",
            "blocks": "minecraft:red_concrete_powder"
          },
          "then": {
            "type": "minecraft:simple_state_provider",
            "state": {
              "Name": "minecraft:clay"
            }
          }
        },
        {
          "if_true": {
            "type": "minecraft:matching_blocks",
            "blocks": "minecraft:netherrack"
          },
          "then": {
            "type": "minecraft:weighted_state_provider",
            "entries": [
              {
                "weight": 5,
                "data": {
                  "Name": "minecraft:tuff"
                }
              },
              {
                "weight": 1,
                "data": {
                  "Name": "minecraft:andesite"
                }
              }
            ]
          }
        },
        {
          "if_true": {
            "type": "minecraft:matching_blocks",
            "blocks": "minecraft:red_terracotta"
          },
          "then": {
            "type": "minecraft:simple_state_provider",
            "state": {
              "Name": "minecraft:cobblestone"
            }
          }
        },
        {
          "if_true": {
            "type": "minecraft:matching_blocks",
            "blocks": "minecraft:red_concrete"
          },
          "then": {
            "type": "minecraft:simple_state_provider",
            "state": {
              "Name": "minecraft:netherrack"
            }
          }
        },
        {
          "if_true": {
            "type": "minecraft:matching_blocks",
            "blocks": "minecraft:redstone_torch"
          },
          "then": {
            "type": "minecraft:simple_state_provider",
            "state": {
              "Name": "minecraft:torch"
            }
          }
        },
        {
          "if_true": {
            "type": "minecraft:matching_blocks",
            "blocks": "minecraft:crimson_roots"
          },
          "then": {
            "type": "minecraft:simple_state_provider",
            "state": {
              "Name": "minecraft:air"
            }
          }
        }
      ]
    },
    "radius": {
      "type": "minecraft:uniform",
      "min_inclusive": 8,
      "max_inclusive": 8
    },
    "half_height": 4,
    "target": {
      "type": "minecraft:matching_blocks",
      "blocks": [
        "minecraft:nether_wart_block",
        "minecraft:crimson_nylium",
        "minecraft:red_concrete_powder",
        "minecraft:netherrack",
        "minecraft:red_terracotta",
        "minecraft:red_concrete",
        "minecraft:redstone_torch",
        "minecraft:weeping_vines",
        "minecraft:sculk_vein",
        "crimson_roots"
      ]
    }
  }
}