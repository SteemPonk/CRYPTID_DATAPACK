execute as @e[type=vindicator, tag=cryptid.mimic, tag=!cryptid.markerapplied] at @s run function cryptid:action/general/spawntickmarker {"name":"tickmimic"}

effect give @e[type=vindicator, tag=cryptid.mimic] weakness infinite 2 true
effect give @e[type=vindicator, tag=cryptid.mimic] invisibility infinite 1 true
execute positioned ~ 400 ~ run effect give @e[type=vindicator, tag=cryptid.mimic,sort=nearest,limit=3] slowness 30 100 true
execute as @e[type=vindicator, tag=cryptid.mimic] at @s run function cryptid:action/stats/mimic


